<section class="card">
  <div class="card__header">
    <h3 class="card__title">{{title}}</h3>
  </div>
  <div class="card__body">
    <figure>
      <object type="image/svg+xml" data="{{{svg}}}"></object>
    </figure>
    
    {{# text}}
    <p>{{.}}</p>
    {{/ text}}
  </div>
  <div class="card__footer"></div>
</section>