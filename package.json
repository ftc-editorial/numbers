{
  "name": "numbers",
  "version": "2.1.0",
  "description": "IG Numbers",
  "private": true,
  "engines": {
    "node": ">=7.8.0"
  },
  "main": "app.js",
  "scripts": {
    "test": "node ./test/bertha-url.js && DEBUG=nums* node ./test/dashboards.js && DEBUG=nums* node ./test/sheet-to-dash.js",
    "postinstall": "bower install",
    "postversion": "git push && git push --tags",
    "get-data": "env DEBUG=nums* node ./model",
    "clean": "rm -rf .tmp/*",
    "build-sass": "node-sass --include-path=bower_components --output-style=compressed client/main.scss ./.tmp/styles/main.css",
    "build-js": "rollup -c",
    "build-html": "NODE_ENV=production node ./utils/build-page.js",
    "build": "npm run build-sass && npm run build-js && npm run build-html",
    "deploy": "cp .tmp/dashboard-china.html ../channel/numbers-china.html"
  },
  "devDependencies": {
    "bower": "^1.8.0",
    "del": "^3.0.0",
    "fs-jetpack": "^1.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-imagemin": "^3.1.1",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.2.0",
    "write-json-file": "^2.0.0"
  },
  "dependencies": {
    "@ftchinese/ftc-footer": "^0.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.5.2",
    "debug": "^2.6.3",
    "got": "^7.0.0",
    "html-minifier": "^3.5.2",
    "inline-source": "^5.2.2",
    "koa": "^2.2.0",
    "koa-logger": "^3.0.0",
    "koa-router": "^7.1.0",
    "koa-static": "^3.0.0",
    "load-json-file": "^2.0.0",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "nunjucks": "^3.0.0",
    "pify": "^3.0.0",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-babili": "^3.1.0",
    "rollup-plugin-bower-resolve": "^0.4.0"
  }
}
